- mc= 'parent-lists relative h-2h75 flex items-center justify-center'
li(class= mc)
	+blockimg('clock', ['平均残業時間（月）'])
		+odomete(['2.4', '時<br>間'], 'items-end justify-center text-blue-option4 mt-1ty' ,'text-5ty8 font-en font-medium', 'text-2ty6 ml-3p')
		+textsub('2018年度', 'mt-2ty5')
	+lineHeight('-right-px')
li(class= mc)
	+blockimg('personal', ['学部卒・院卒割合'])
		ul.lists-inner.flex.justify-center.mt-1ty8
			li(class="last:mr-0 mr-1ty5")
				+odomete(['00', '%'], 'text-blue-custom1 font-en font-medium items-end' ,'text-4ty5', 'text-2ty5 mb-2p', '学<br>部', 'flex items-center justify-start', 'flex items-center text-white h-6ty px-3p mr-1ty bg-blue-custom1')
			li(class="last:mr-0 mr-1ty5")
				+odomete(['00', '%'], 'text-blue-option2 font-en font-medium items-end' ,'text-4ty5', 'text-2ty5 mb-2p', '院<br>卒', 'flex items-center justify-start', 'flex items-center text-white h-6ty px-3p mr-1ty bg-blue-option2')
	+lineHeight('-right-px')
li(class= mc)
	+blockimg(false, ['育休取得後復帰率'])
		+odomete(['00', '%'], 'items-end justify-center text-orange-option1 font-en font-medium mt-1ty' ,'text-6ty7', 'text-2ty5 mb-2p')
		+textsub('2018年度', 'mt-1ty5')
	+lineHeight('-right-px')
li(class= mc)
	+blockimg('child', ['産休・育休取得率'])
		+odomete(['100', '%'], 'items-end justify-center text-pink-option1 font-en font-medium mt-7p' ,'text-6ty6', 'text-2ty5 mb-2p')
		+textsub('2014年～2018年', 'mt-2ty')
